<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="view_inherit_purchase_order_form" model="ir.ui.view">
            <field name="name">view.inherit.purchase.order.form</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='order_line']//list/field[@name='price_subtotal']" position="after">
                    <field name="on_hand" optional="show"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>